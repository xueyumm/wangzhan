*{
	padding:0;
	margin:0;
	list-style: none;
	text-decoration: none;
}
/*导航条*/
.head{
	width: 1200px;
	height:100px;
	margin:0 auto;
}
.header_right ul li{
	float: left;
	width: 100px;
	height: 100px;
	text-align: center;
	line-height: 100px;
}
.header_right ul li a{
	color: #000;
}
.header_right ul li a:hover{
	color: red;
}
.header_right .header_left_name{
	width: 300px;
	height: 100px;
	text-align: center;
	line-height: 100px;
	font-size: 40px;
}
.header_right .header_left_name a{
	color: #012877;
}
.one a{
	padding-bottom:5px;
	border-bottom:1px solid red;
}
.images_bg img{
	width:100%;
}

/*白色背景的标题*/
.content_one{
	width: 1200px;
	height: 600px;
	margin:0 auto;
}
.content_one_top{
	width: 1200px;
	height: 168px;
	margin:0 auto;
}
.content_one_title{
	width: 320px;
	height:168px;
	margin:0 auto;
}
.english_tit{
	width: 320px;
	height: 100px;
	text-align: center;
	line-height: 100px;
}
.english_tit1{
	width: 160px;
	height: 100px;
	float: left;
	font-size: 35px;
	color: #343434;
}
.english_tit2{
	width: 160px;
	height: 100px;
	float: right;
	font-size: 35px;
	color: #fdabad;
}
.english_futit{
	width: 320px;
	height: 68px;
	color: #333;
	font-size: 24px;
	text-align: center;
}
.english_futit span{
	color: #fdabad;
	padding:0 10px;
}

/*四张图片*/
.content_one_cont{
	width: 1200px;
	height: 400px;
}
.content_one_cont ul li{
	width: 270px;
	height: 200px;
	float: left;
}
.images_img1{
	margin-left: 40px;
}
.images_img2{
	margin:0 40px;
}
.content_one_text li{
	width:270px;
	height: 170px;
	float: left;
	overflow: hidden;
}
.text_txt1{
	margin-left: 40px;
}
.text_txt2{
	margin:0 40px;
}
.content_one_text li span{
	display: block;
	font-size: 18px;
	color: #333;
	width: 270px;
	height: 30px;
	text-align: center;
	line-height: 30px;
}
.content_one_text li p{
	font-size: 13px;
	color: #9a9899;
}

/*黑色背景的标题*/
.content_two_bg{
	width: 100%
	height:168px;
	background: #4d6071;
}
.lish_tit1{
	width: 160px;
	height: 100px;
	float: left;
	font-size: 35px;
	color: #fff;
}
.lish_futit{
	width: 320px;
	height: 68px;
	color: #fff;
	font-size: 24px;
	text-align: center;
}
.lish_futit span{
	color: #fdabad;
	padding:0 10px;
}

/*100%铺满的图片*/
.content_images{
	width: 100%;
	height: 417px;
}
.content_images ul li{
	width: 20%;
	float: left;
}
.content_images ul li img{
	width: 100%;
}

/*网站设计理念的解释*/
.content_explain{
	width: 1200px;
	height: 150px;
	background: #fff;
	line-height: 150px;
	margin:0 auto;
	
}
.content_explain_text{
	width: 700px;
	height: 100px;
	margin:0 auto;
	padding-top: 25px;
	line-height: 100px;
}
.content_explain_text p{
	width: 700px;
	height: 30px;
	font-size: 14px;
	color: #333;
	line-height: 30px;
	text-align: center;
}

